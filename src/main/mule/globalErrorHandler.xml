	<error-handler name="global-error-handler" doc:id="dbda4398-3caf-4074-b0aa-0e37fb5c80ef" >
		<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="23e5a881-d2bb-46f7-b754-c2bfd9762f57" type="ANY">
			<ee:transform doc:name="error payload" doc:id="085855c0-6bb8-4b7d-a568-2ca80322328b" >
				<ee:message >
					<ee:set-payload resource="dwl/technicalError.dwl" />
				</ee:message>
				<ee:variables >
				</ee:variables>
			</ee:transform>
			<logger level="ERROR" doc:name="Logger" doc:id="44adf8d7-0ad9-4ebd-9e2b-5adadd4d2643" message="#[payload]"/>
		
</on-error-continue>
	
</error-handler>

</mule>
